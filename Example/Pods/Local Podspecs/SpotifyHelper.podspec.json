{
  "name": "SpotifyHelper",
  "version": "0.1.7",
  "summary": "The spotify sdk, with helper classes",
  "description": "The spotify sdk, with helper classes.",
  "homepage": "https://github.com/balazsmorv/SpotifyHelper",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "balazsmorv": "balazsmorvay@yahoo.com"
  },
  "source": {
    "git": "https://github.com/balazsmorv/SpotifyHelper.git",
    "tag": "0.1.7"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2"
  ],
  "static_framework": true,
  "source_files": "SpotifyHelper/Classes/**/*",
