  "vendored_frameworks": "ios-sdk/SpotifyiOS.framework",
  "prepare_command": "./prepare-iOS-SDK.sh",
  "frameworks": "UIKit",
  "dependencies": {
    "RxSwift": [
      "~> 5.0"
    ],
    "RxRelay": [
      "~> 5.0"
    ],
    "RxCocoa": [
      "~> 5.0"
    ],
    "RxGesture": [
      "~> 3.0"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.2"
}
